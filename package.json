    "@mui/material": "^5.6.0",
    "@mui/x-data-grid": "^5.9.0",
    "@reduxjs/toolkit": "^1.8.1",
    "@tanstack/react-table": "alpha",
    "@tauri-apps/api": "^1.0.0-rc.3",
    "ag-grid-community": "^27.2.1",
