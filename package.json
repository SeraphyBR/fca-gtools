    "vite": "^2.9.1",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "vite": "vite",
    "tauri": "tauri",
    "flask:serve": "sh -c 'source ./src-flask/.venv/bin/activate && python src-flask/main.py'",
    "dev:all": "concurrently 'yarn:vite' 'yarn:flask:serve' 'yarn:tauri dev'",
    "cypress:open": "cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.0-rc.7",
    "@types/node": "^12.0.0",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
