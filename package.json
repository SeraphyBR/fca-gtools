    "axios": "^0.26.0",
    "husky": "^7.0.4",
    "i18next": "^21.6.16",
    "lint-staged": "^12.1.4",
    "notistack": "^2.0.4",
    "prettier": "^2.5.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-dropzone": "^12.0.4",
    "react-flow-renderer": "^10.1.0",
    "react-i18next": "^11.16.6",
    "react-redux": "^8.0.0",
    "react-router-dom": "6",
